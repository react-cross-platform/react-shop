    "formik": "^0.9.4",
    "graphql-tag": "^2.4.2",
    "history": "^4.6.3",
    "immutability-helper": "^2.2.0",
    "path-to-regexp": "^2.0.0",
    "query-string": "^5.0.1",
    "raw-loader": "^0.5.1",
    "react": "16.0.0",
    "react-apollo": "1.4.15",
    "react-dom": "16.0.0",
    "react-fastclick": "^3.0.1",
    "react-google-tag-manager": "^2.2.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-masonry-infinite": "^1.1.0",
    "react-redux": "^5.0.4",
    "react-render-html": "^0.5.2",
    "react-router": "4.2.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-sidebar": "^2.3.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.0",
    "redux-thunk": "^2.2.0",
    "yup": "^0.22.0"
  },
  "devDependencies": {
    "@types/es6-promise": "^0.0.32",
