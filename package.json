    "@types/redux-logger": "3.0.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "~1.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
